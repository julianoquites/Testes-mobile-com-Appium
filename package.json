{
  "name": "ebac-store-mobile-tests",
  "type": "module",
  "devDependencies": {
    "@wdio/allure-reporter": "^8.39.0",
    "@wdio/cli": "^9.2.14",
    "@wdio/local-runner": "^9.2.15",
    "@wdio/mocha-framework": "^8.39.0",
    "@wdio/spec-reporter": "^8.39.0",
    "@wdio/sauce-service": "^8.0.0",
    "appium": "^2.0.0",
    "appium-uiautomator2-driver": "^3.7.4",
    "appium-xcuitest-driver": "^5.0.0",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "clean": "rimraf allure-results/ allure-report/ screenshots/",
    "pretest": "npm run clean",
    "test": "cross-env FORCE_COLOR=1 wdio run ./wdio.conf.js",
    "test:debug": "cross-env DEBUG=1 FORCE_COLOR=1 wdio run ./wdio.conf.js",
    "test:sauce": "cross-env ENV=sauce wdio run ./wdio.conf.js",
    "report": "allure generate allure-results --clean && allure open",
    "report:ci": "allure generate allure-results --clean"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "allure-commandline": "^2.30.0",
    "dotenv": "^16.4.5"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
